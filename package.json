{
  "name": "effso",
  "version": "0.4.0",
  "type": "module",
  "packageManager": "pnpm@7.27.1",
  "description": "Effso is a tool to help you operate files and directories in a more efficient way.",
  "keywords": [
    "CLI",
    "effso",
    "file system",
    "file"
  ],
  "license": "MIT",
  "author": "alexzhang1030",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/effso.git"
  },
  "scripts": {
    "play": "tsx ./src/cli.ts",
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish",
    "dep:up": "taze -I major"
  },
  "files": [
    "dist",
    "template",
    "bin"
  ],
  "bin": {
    "effso": "./bin/effso.mjs"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.35.2",
    "@clack/core": "^0.1.7",
    "@clack/prompts": "^0.4.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.0",
    "bumpp": "^8.2.1",
    "colorette": "^2.0.19",
    "eslint": "^8.34.0",
    "home-or-tmp": "^4.0.0",
    "lint-staged": "^13.1.2",
    "pathe": "^1.1.0",
    "pkg-dir": "^7.0.0",
    "putil-merge": "^3.10.3",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.8.5",
    "tsx": "^3.12.3",
    "typescript": "4.9.5",
    "unbuild": "^1.1.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^2.15.3",
    "esbuild": "^0.17.9",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0"
  }
}
